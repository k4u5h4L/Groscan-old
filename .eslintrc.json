{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": ["plugin:react/recommended", "next/core-web-vitals"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "@typescript-eslint"],
    "rules": {
        "react/prop-types": 0,
        "import/no-anonymous-default-export": 0,
        "@next/next/no-css-tags": 0,
        "@next/next/no-sync-scripts": 0,
        "@next/next/no-img-element": 0,
        "jsx-a11y/alt-text": 0
    }
}
